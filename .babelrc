{
  "exclude": "node_modules/**",
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true,
        "shippedProposals": true
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    ["module-resolver", { "root": ["./"] }],
    "@babel/plugin-transform-runtime",
    "babel-plugin-styled-components"
  ]
}
